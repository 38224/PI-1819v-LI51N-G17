<table class="table table-hover">
    <thead>
        <tr>    
            <th scope="col">Nome</th>
            <th scope="col">URL Name</th>
            <th scope="col">Duração</th>
            <th scope="col">Nome Artista</th>
            <th scope="col">URL Artista</th> 
			<th scope="col">Remover</th> 
        </tr>
        <tbody>
            {{#each .}}
                <tr scope="row">
                    <td>{{name}}</td>
                    <td>{{url}}</td>
                    <td>{{duration}}</td>
					<td><a href="#artists/{{artist.name}}">{{artist.name}}</a></td>
					<td>{{artist.url}}</td>
					<td><button id="removeMusic_{{name}}">X</button></td>
                </tr>
            {{/each}}
        </tbody>
    </thead>
</table>