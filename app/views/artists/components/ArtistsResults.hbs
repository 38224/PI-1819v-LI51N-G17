<table class="table table-hover">
    <thead>
        <tr>    
            <th scope="col">Nome</th>
            <th scope="col">URL</th>
            <th scope="col">Ouvintes</th>
        </tr>
        <tbody>
            {{#each this}}
                <tr scope="row">
					<td><a href="#artists/{{mbid}}/albums">{{name}}</a></td>
                    <td><a href="{{url}}" target="_blank">{{url}}</a></td>
                    <td>{{listeners}}</td>
                </tr>
            {{/each}}
        </tbody>
    </thead>
</table> 